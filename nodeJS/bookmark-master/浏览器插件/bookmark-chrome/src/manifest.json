{
  "name": "bookmark-chrome",
  "description": "A Vue.js web extension",
  "version": null,
  "manifest_version": 2,
  "permissions": [
    "contextMenus"
  ],
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "bookmark-chrome",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  },
  "content_scripts": [{
      "matches": ["*://*/*"],
      "js": ["static/sso.js"]
    }

  ]
}
